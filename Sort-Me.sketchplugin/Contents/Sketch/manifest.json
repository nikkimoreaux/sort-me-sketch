{
  "name": "Sort Me",
  "description": "Sort artboards and layers by name",
  "author": "Roman Shamin / Nikki Moreaux",
  "homepage": "https://github.com/nikkimoreaux/sort-me-sketch",
  "version": 1.4,
  "identifier": "com.github.romashamin.sort-me-sketch",
  "appcast": "https://raw.githubusercontent.com/nikkimoreaux/sort-me-sketch/master/appcast.xml",
  "compatibleVersion": 3,
  "bundleVersion": 1,
  "commands": [
    {
      "name": "Sort Top → Bottom",
      "identifier": "sort-ascending",
      "shortcut": "ctrl shift o",
      "script": "Sort-Me.coscript",
      "handler": "runAscending"
    },
    {
      "name": "Sort Bottom → Top",
      "identifier": "sort-descending",
      "shortcut": "ctrl shift p",
      "script": "Sort-Me.coscript",
      "handler": "runDescending"
    }
  ],
  "menu": {
    "items": [
      "sort-ascending",
      "sort-descending"
    ]
  }
}
